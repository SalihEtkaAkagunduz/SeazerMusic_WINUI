                        <Button Width="300"   Height="100"
        Padding="0"
        BorderThickness="0"
        HorizontalContentAlignment="Stretch"
        VerticalContentAlignment="Stretch">

                            <Button.Template  >
                                <ControlTemplate TargetType="Button">
                                    <Grid>
                                        <!-- Köşeleri gerçekten kesmek için ClipGeometry -->
                                        <Grid.Clip>
                                            <RectangleGeometry   
                               Rect="0,0,300,170"/>
                                        </Grid.Clip>

                                        <!-- Arka plan resmi -->
                                        <Border Background="Transparent" CornerRadius="10">
                                            <Image x:Name="BackgroundImage"
               Source="/Assets/listimages/b1.jpg"
               Stretch="UniformToFill"/>

                                        </Border>


                                        <!-- Karartma -->
                                        <Border Background="#20000000" CornerRadius="10"/>

                                        <!-- Hover & Pressed efekti -->
                                        <Frame x:Name="Overlay" Background="Transparent" CornerRadius="10"/>

                                        <!-- Metin -->
                                        <ContentPresenter HorizontalAlignment="Stretch"
VerticalAlignment="Bottom"
Content="Lokal" Margin="15,0,0,15"
Foreground="White"
FontSize="16"
FontWeight="Bold"/>

                                        <!-- Visual States -->
                                        <VisualStateManager.VisualStateGroups>
                                            <VisualStateGroup x:Name="CommonStates">
                                                <VisualState x:Name="Normal"/>
                                                <VisualState x:Name="PointerOver">
                                                    <Storyboard>
                                                        <ColorAnimation Storyboard.TargetName="Overlay"
                                        Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)"
                                        To="#33FFFFFF" Duration="0:0:0.1"/>
                                                    </Storyboard>
                                                </VisualState>
                                                <VisualState x:Name="Pressed">
                                                    <Storyboard>
                                                        <ColorAnimation Storyboard.TargetName="Overlay"
                                        Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)"
                                        To="#55FFFFFF" Duration="0:0:0.1"/>
                                                    </Storyboard>
                                                </VisualState>
                                            </VisualStateGroup>
                                        </VisualStateManager.VisualStateGroups>
                                    </Grid>
                                </ControlTemplate>

                            </Button.Template>
                        </Button>

                    </Viewbox>
